{
    "Diablo2Directory": "C:\\Program Files (x86)\\Diablo II",
    "Kestrel": {
        "Limits": {
            "MaxConcurrentConnections": 100,
            "MaxConcurrentUpgradedConnections": 100
        },
        "EndPoints": {
            "Http2": {
                "Url": "http://[::]:5000",
                "Protocols": "Http2"
            }
        }
    },
    "Logging": {
        "Debug": {
            "IncludeScopes": false
        },
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Grpc.AspNetCore.Server": "Warning",
            "Microsoft.Extensions.Hosting": "Warning"
        }
    }
}